{
    "name": "raccoon-city-graphql",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
        "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
        "build": "rimraf dist && tsc",
        "precommit": "lint-staged",
        "prettier": "pretty-quick"
    },
    "dependencies": {
        "bcryptjs": "2.4.3",
        "graphql-yoga": "1.18.3",
        "jsonwebtoken": "8.5.1",
        "lint-staged": "^9.4.3",
        "pretty-quick": "^2.0.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "2.4.2",
        "@types/node": "10.17.5",
        "dotenv-cli": "1.4.0",
        "lint-staged": "^9.4.3",
        "nodemon": "1.19.4",
        "prettier": "^1.19.1",
        "prisma": "1.34.10",
        "rimraf": "2.7.1",
        "ts-node": "7.0.1",
        "typescript": "3.2.2"
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier",
            "git add"
        ]
    }
}
